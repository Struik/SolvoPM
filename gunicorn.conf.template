[program:gunicorn]
command=/home/django/.pyenv/versions/telegram_bot/bin/gunicorn SolvoPm.wsgi:application -b 127.0.0.1:8001 -w 1 --timeout=60 --graceful-timeout=60 --max-requests=1024
directory=/home/django/SolvoPM/
user=django
redirect_stderr=True
stdout_logfile=/home/django/tmp/gunicorn.log
stderr_logfile=/home/django/tmp/gunicorn_err.log
autostart=true
autorestart=true
startsecs=10
stopwaitsecs=10
priority=999